<% content_for :title do %>
Add member
<% end %> 

<%= error_messages_for :member %>

<% form_for :member, @member, :url => { :action => :create }, :html => { :multipart => true } do |f| %>
<%= render :partial => "form", :locals => { :f => f } %>
<br/>

<div id="commit">
<%= submit_tag "Create" %>
</div>

<% end -%>
